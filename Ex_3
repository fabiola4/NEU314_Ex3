using PyPlot;
imgE_C = imread("el-capitan.png");
imshow(imgE_C);
size(imgE_C);


function extractRbg(filename)
    imgFunction = imread(filename)
    imshow(imgFunction)
    imgRed = imgFunction[:, :, 1]
    imgBlue = imgFunction[:, :, 2]
    imgGreen = imgFunction[:, :, 3]
    return imgRed, imgBlue, imgGreen
    println("Reset with git reset --mixed")
end


r,g,b = extractRbg("el-capitan.png");


allColors = hcat(g, b, r);
image2 = reshape(allColors, 360, 640, 3);
subplot(1,2,1);
imshow(imgE_C);
subplot(1,2,2);
imshow(image2);
axis("off");
suptitle("Original RBG Image and GBR Version");
